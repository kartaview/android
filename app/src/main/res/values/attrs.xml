        value="0x08"/>
    <flag name="clip_vertical"
        value="0x80"/>
    <flag name="end"
        value="0x00800005"/>
    <flag name="fill"
        value="0x77"/>
    <flag name="fill_horizontal"
        value="0x07"/>
    <flag name="fill_vertical"
        value="0x70"/>
