<?xml version="1.0" encoding="utf-8"?>
<resources>
  <attr name="gravity">
    <flag name="bottom"
        value="0x50"/>
    <flag name="center"
        value="0x11"/>
    <flag name="center_horizontal"
        value="0x01"/>
    <flag name="center_vertical"
        value="0x10"/>
    <flag name="clip"
        value="0x88"/>
